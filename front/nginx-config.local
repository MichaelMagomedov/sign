server {
    listen 80;
    listen [::]:80;
    root /var/www/sign/front;
    index /html/auth.tpl.html;
 
    location /auth {
        default_type "text/html";
        try_files $uri.html $uri $uri/ /html/auth.tpl.html;
    }
   
    location /admin {
        default_type "text/html";
        try_files $uri.html $uri $uri/ /html/admin.tpl.html;
    }
 
    location ~* \.(html|js|jpg|png|css)$ {
        root /var/www/sign/fron/compile;
        expires 30d;
    }
}
